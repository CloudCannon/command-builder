  "repository": {
    "type": "git",
    "url": "git+https://github.com/CloudCannon/command-builder"
  },
  "keywords": [
    "SSG",
    "static",
    "site",
    "generator",
    "build"
  ],
  "files": [
    "/index.js",
    "/helpers",
    "/options",
    "/lib/compiler.js",
    "/lib/eleventy.js",
    "/lib/hugo.js",
    "/lib/jekyll.js",
    "/lib/reader.js",
    "/lib/reader-npm.js",
    "/lib/reseed.js",
    "/lib/rosey.js",
    "/lib/static.js"
  ],
  "author": "Nathan Kennedy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CloudCannon/command-builder/issues"
  },
  "homepage": "https://github.com/CloudCannon/command-builder#readme",
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-cloudcannon": "^1.0.0-rc5",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-mocha": "^8.1.0",
    "jest": "^27.0.6"
  }
